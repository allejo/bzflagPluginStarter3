{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "semi": true,
  "importOrder": [
    "^(?!\\.+\\/).*(?<!\\.(s?css))$",
    "^[./]+[^.]+$",
    "^\\w.*\\.[a-z]+$",
    "\\.[a-z]+$"
  ],
  "importOrderSeparation": true,
  "experimentalBabelParserPluginsList" : ["jsx", "typescript"]
}
